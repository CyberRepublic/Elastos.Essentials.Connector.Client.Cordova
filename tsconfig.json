{
    "compilerOptions": {
      "esModuleInterop": true,
      "declaration": true,
      "outDir": "dist",
      "module": "es2020",
      "target": "es2020",
      "moduleResolution": "node",
      "typeRoots": [
        "node_modules",
      ],
      "sourceMap": true,
      "types": [
        "web3-core/types",
        "elastos-cordova-plugin-did",
        "elastos-cordova-plugin-hive",
        "elastos-cordova-plugin-intent"
      ]
    },
    "include": ["src/**/*"],
    "exclude": ["dist/*", "dist.esm/*"]
}